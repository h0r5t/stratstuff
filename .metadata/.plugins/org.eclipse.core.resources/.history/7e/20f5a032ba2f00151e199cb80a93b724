package stratstuff;

import java.awt.Color;
import java.awt.Graphics2D;

public class UnitSelectionMarker implements Drawable {

	public UnitSelectionMarker() {

	}

	@Override
	public void draw(Graphics2D g, int xinpixels, int yinpixels) {
		g.setColor(Color.CYAN);
		g.drawOval(xinpixels, yinpixels, 30, 30);
	}

}
